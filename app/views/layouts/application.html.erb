<!DOCTYPE html>
<html>
  <head>
    <title>SambaFm</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="container">
      <%= render 'shared/nav' %>
      <div class = "songs_table">
        <% flash.each do |k, v| %>
          <div class="flash <%= k %>"><%= v %></div>
        <% end %>
        <%= yield %>
      </div>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>
